{
  "name": "server",
  "version": "1.0.0",
  "scripts": {
    "start:server:dev": "cd backend && npm run dev",
    "start:client:dev": "cd frontend && npm run dev",
    "build:server": "cd backend && npm run build",
    "start:server": "cd backend && npm run start",
    "build:client": "cd frontend && npm run build",
    "dev": "concurrently \"npm run start:server:dev\" \"npm run start:client:dev\"",
    "prod": "npm run build:client && npm run build:server && npm run start:server",
    "pm2prod": "npm run build:client && npm run build:server && pm2 start backend/ecosystem.config.cjs --env production",
    "install:all": "node install-all.js"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "type": "module",
  "devDependencies": {
    "concurrently": "^9.2.1",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.9.3"
  }
}
